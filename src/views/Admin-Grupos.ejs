	<section class="ftco-section ftco-wrap-about ftco-no-pb ftco-no-pt" id="registroG">
		<div class="container">
			<div class="row no-gutters">
				<div class="col-sm-4 p-4 p-md-5 col-lg-5 d-flex align-items-center justify-content-center bg-success">
					<form action="/grupoAdd" method="POST" class="appointment-form">
						<br>
						<h3 class="mb-3">Registro Grupos</h3>
						<div class="row">
							<div class="col-md-6">
								<label style="color: white;">Nro Grupo</label>

							</div>
							<div class="col-md-5">
								<div class="form-group">
									<input type="number" class="form-control" placeholder="12" name="numGrupo"
										required="Ingrese el nro de grupo">
								</div>
							</div>
							
							
							<div class="col-md-12">
								<div class="form-group">
									<label style="color: white;">Seleccionar asignatura</label>

									<select class="form-control" id="exampleFormControlSelect1" name="codigoAsignatura">

										<% for(var i = 0; i < asignatura.length; i++){ %>
										<option style="background-color:#52D976"
											value="<%= asignatura[i]._id  %>">
											<%= asignatura[i].nombreASignatura %></option>



										<% } %>

									</select>
								</div>
							</div>
							<div class="col-md-12">
								<div class="form-group">
									<label style="color: white;">Seleccionar Docente</label>

									<select class="form-control" id="exampleFormControlSelect1" name="codigoDocente">

										<% for(var i = 0; i < docente.length; i++){ %>
										<option style="background-color:#52D976"
											value="<%= docente[i]._id  %>"><%= docente[i].nombres %></option>



										<% } %>

									</select>

								</div>
							</div>
							<div class="col-md-12">
								<div class="form-group">
									<label style="color: white;">Seleccionar estudiante</label>
									<div class="container">
										<div class="row">
											<div class="col">
												<select class="form-control" name="estudiante" id="selEst">

													<% for(var i = 0; i < estudiante.length; i++){ %>
													<option style="background-color:#52D976"
														value="<%= estudiante[i].codigoEstudiante  %>">
														<%= estudiante[i].nombres %></option>



													<% } %>
												</select>
											</div>
											<input id="estudiantesJS" name="tempEsta" type="hidden">

											<br><br><br>
											<script>

												var gt;
										
												function creat(){
													gt =[];
												}
										
												function verJs(){
													alert("JS " + JSON.stringify(gt))
												}
										
												function bot() {
													alert("Jajajajaj");
													tex = document.getElementById("selEst").options[document.getElementById("selEst").selectedIndex].value;
													gt.push({"estudiante" : tex});
													alert("selEst" + gt[0].estudiante);
													document.getElementById("estudiantesJS").value = JSON.stringify(gt)
										
										
												}
											</script>
										


											<div class="col">

												
												
												<button class="btn btn-danger" onclick="bot()" type="button">Añadir</button>
												

												

											</div>
										</div>
									</div>
								</div>
							</div>
							<div class="col-md-6">
								<label style="color: white;">Nro Cupos</label>

							</div>
							<div class="col-md-5">
								<div class="form-group">
									<input type="number" class="form-control" placeholder="cupos" value="10"
										name="cantCupos" required="Ingrese la cantidad de cupos">
								</div>
							</div>

							<div class="col-md-2">
							</div>
							<div class="col-md-8">
								<div class="form-group">
									<input type="submit" value="Registrar" class="btn btn-white py-3 px-4">
								</div>
							</div>
						</div>
					</form>
				</div>
				<div class="col-sm-8 wrap-about py-5 col-lg-7 ftco-animate img"
					style="background-image: url(/static/images/grupos.jpg);">
					<div class="row pb-5 pb-md-0">
						<div class="col-md-12 col-lg-7">
							<div class="heading-section mt-5 mb-4">
								<div class="pl-lg-3 ml-md-5">
									<span class="subheading" style="color:#a1c933; size: 14px">Información
										Grupos</span>
								</div>
							</div>
							<div class="pl-lg-3 ml-md-5">
								<p>
									<FONT size=4 COLOR="white">
										En esta seccion se añaden los grupos para las asignaturas exitentes, se le
										asignan
										a su vez al docente y al estudiante relacionados con la misma.

									</FONT>

								</p>
							</div>
