	<section id="modificar" class="ftco-section testimony-section"
		style="background-image: url(/static/images/modificar.jpg);">
		<div class="overlay"></div>
		<div class="container">
			<div class="row justify-content-center mb-3 pb-2">
				<div class="col-md-10 text-center heading-section heading-section-white ftco-animate">
					<form action="/docenteBuscar" class="appointment-form" method="POST">
						<br>
						<div class="row">
							<div class="col-4">
							</div>
							<div class="col-3">
								<input class="form-control mr-sm-2" type="search" placeholder="Ingresar código docente"
									aria-label="Search" name="codigoDocente">
							</div>
							<div class="col-2">
								<input type="submit" value="BUSCAR" class="btn btn-white py-2 px-3">

							</div>
						</div>
						<br><br>
					</form>



					<div class="col-md-10 text-center heading-section heading-section-white ftco-animate">
						<form action="/docenteEdit" class="appointment-form" method="POST">


							<div class="row">
								<div class="col-5">
								</div>
								<h3 class="col-mb-7	">Modificar Docentes</h3>
								<input id="" name="codigoDocente" type="hidden" value="<%= docente.codigoDocente  %>">
							</div>

							<div class="row">

								<div class="col-4">
								</div>
								<div class="col-6">
									<br>

									<div class="form-group">
										<input type="text" class="form-control" placeholder="Nombres" name="nombres"
											value=" <%= docente.nombres  %> ">
									</div>
								</div>
								<div class="col-2">
								</div>

								<div class="col-4">
								</div>
								<div class="col-6">
									<div class="form-group">
										<input type="text" class="form-control" placeholder="Apellidos" name="apellidos"
											value=" <%= docente.apellidos  %> ">
									</div>
								</div>
								<div class="col-2">
								</div>
								<div class="col-4">
								</div>
								<div class="col-6">
									<div class="form-group">
										<input type="email" class="form-control" placeholder="Email" name="correo"
											value=" <%= docente.correo  %> ">
									</div>
								</div>

								<div class="col-2">
								</div>
								<div class="col-4">
								</div>
								<div class="col-3">
									 <br>

										<input type="submit" value="Actualizar" class="btn btn-white py-2 px-3">

								</div>
						</form>
						<div class="col-4">
							<br>
							<a class="btn btn-primary" href="/docenteDelete/<%= docente.codigoDocente  %>"
								role="button">Eliminar</a>
						</div>
					</div>
					<br>

				</div>


			</div>

		</div>
	</section>




