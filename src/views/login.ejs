<body>
	
	
	<div class="container-login100" style="background-image: url(/static/images/segundofondo.jpg);">
		<div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30">
			<form class="login100-form validate-form">
				<img src="/static/images/logo.png" class="img-fluid" alt="Responsive image">
				<span class="login100-form-title p-b-37">
					Iniciar Sesión
				</span>

				
				<div class="wrap-input100 validate-input m-b-20" data-validate="Enter username or email">
					<input class="input100" type="text" name="correo" placeholder="Correo">
					<span class="focus-input100"></span>
				</div>

				<div class="wrap-input100 validate-input m-b-25" data-validate = "Enter password">
					<input class="input100" type="password" name="contraseña" placeholder="Contraseña">
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100 validate-input m-b-25">
					
					<select class="form-control" id="exampleFormControlSelect1" name="usuario">
						<option style="background-color:burlywood" value="Administrador">Administrador</option>
							<option style="background-color:burlywood" value="Profesor">Profesor</option>
								<option style="background-color:burlywood" value="Estudiante">Estudiante</option>

					</select>
				</div>



				<div class="container-login100-form-btn">
					<br><br>
					<button class="login100-form-btn">
						Ingresar
					</button>
				</div>
			

				
			</form>

			
		</div>
	</div>
	
	

	<div id="dropDownSelect1"></div>
	
